{
  "name": "@carforyou/configuration",
  "version": "0.0.0",
  "description": "CAR FOR YOU configuration conventions",
  "scripts": {
    "version": "npm run build",
    "build": "pika build",
    "lint": "eslint --ext js,json .",
    "format": "npm run lint -- --fix",
    "test": "jest",
    "test:debug": "node --inspect-brk --inspect=127.0.0.1:9229 ./node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carforyou/carforyou-configuration-pkg.git"
  },
  "author": "CAR FOR YOU",
  "license": "MIT",
  "homepage": "https://github.com/carforyou/configuration-pkg#readme",
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-modules-commonjs": "7.16.0",
    "@carforyou/eslint-config": "3.1.22",
    "@pika/pack": "0.5.0",
    "@pika/plugin-build-node": "0.9.2",
    "@pika/plugin-standard-pkg": "0.9.2",
    "@types/jest": "27.0.3",
    "jest": "27.3.1",
    "semantic-release": "17.4.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "dependencies": {
    "dotenv": "10.0.0",
    "es-abstract": "1.19.1",
    "typescript": "4.5.2"
  }
}
